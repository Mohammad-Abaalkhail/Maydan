{
  "name": "almaydan-ya-7maidan",
  "version": "1.0.0-milestone",
  "description": "Online Arabic multiplayer card game - الميدان يا حميدان",
  "private": true,
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "test:backend": "cd backend && npm run test:socket",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "cd backend && npm run prisma:migrate",
    "db:seed": "cd backend && npm run prisma:seed"
  },
  "keywords": [
    "arabic",
    "card-game",
    "multiplayer",
    "kuwait",
    "socket.io",
    "react"
  ],
